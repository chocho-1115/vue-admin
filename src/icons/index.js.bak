const context = import.meta.glob('./svg/*', {
  eager: true,
  query: '?component',
})

const res = {}

for (const path in context) {
  let [fileName, fileType] = path.match(/([^/]+)$/)[0].split('.')
  if (fileType !== 'svg') continue
  res[fileName] = context[path]
}

console.log(res)
export default res

